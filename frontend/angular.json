{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "www",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              "src/theme/variables.scss",
              "src/global.scss",
              "src/theme/styles.scss",
              "node_modules/primeflex/primeflex.css",
              {
                "input": "src/theme/theme-bootstrap4-dark-yellow.scss",
                "bundleName": "bootstrap4-dark-yellow",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-bootstrap4-light-purple.scss",
                "bundleName": "bootstrap4-light-purple",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-bootstrap4-dark-purple.scss",
                "bundleName": "bootstrap4-dark-purple",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-bootstrap4-light-blue.scss",
                "bundleName": "bootstrap4-light-blue",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-bootstrap4-dark-blue.scss",
                "bundleName": "bootstrap4-dark-blue",
                "inject": false
              },


              {
                "input": "src/theme/primeng-themes/theme-arya-blue.scss",
                "bundleName": "arya-blue",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-arya-orange.scss",
                "bundleName": "arya-orange",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-arya-purple.scss",
                "bundleName": "arya-purple",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-arya-green.scss",
                "bundleName": "arya-green",
                "inject": false
              },


              {
                "input": "src/theme/primeng-themes/theme-md-light-indigo.scss",
                "bundleName": "md-light-indigo",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-md-dark-indigo.scss",
                "bundleName": "md-dark-indigo",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-md-dark-deeppurple.scss",
                "bundleName": "md-dark-deeppurple",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-md-light-deeppurple.scss",
                "bundleName": "md-light-deeppurple",
                "inject": false
              },


              {
                "input": "src/theme/primeng-themes/theme-mdc-light-indigo.scss",
