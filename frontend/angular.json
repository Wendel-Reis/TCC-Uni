              },
              {
                "input": "src/theme/primeng-themes/theme-lara-dark-teal.scss",
                "bundleName": "lara-dark-teal",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-lara-light-blue.scss",
                "bundleName": "lara-light-blue",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-lara-light-indigo.scss",
                "bundleName": "lara-light-indigo",
                "inject": false
              },
              {
                "input": "src/theme/primeng-themes/theme-lara-light-purple.scss",
                "bundleName": "lara-light-purple",
