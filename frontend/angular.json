              },
              {
                "input": "src/theme/primeng-themes/theme-lara-light-teal.scss",
                "bundleName": "lara-light-teal",
                "inject": false
              },

              
              {
                "input": "src/theme/primeng-themes/theme-luna-amber.scss",
                "bundleName": "luna-amber",
