    "@nestjs/platform-socket.io": "^9.1.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.1",
    "@nestjs/terminus": "^9.2.2",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.1.1",
    "aws-sdk": "^2.1209.0",
    "axios": "^1.4.0",
